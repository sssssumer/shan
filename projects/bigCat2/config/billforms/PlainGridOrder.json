{
    "formTemplate": "normal",
    "head": {
        "type": "element",
        "elementType": "Header",
        "elementProps": {
            "canBack": true,
            "titleElement": {
                "type": "element",
                "elementType": "FormTitle",
                "elementProps": {
                    "containerStyle": {
                        "alignItems": "center",
                        "justifyContent": "center"
                    }
                }
            }
        }
    },
    "items": [
        "DocumentNumber",
        "DocumentDate",
        "MM_DocumentTypesID",
        "PurOrganizationID",
        "PurchasingGroupID",
        "CompanyCodeID",
        "VendorID",
        "grid0"
    ],
    "action": {
        "type": "element",
        "elementType": "GridActionButton",
        "elementProps": {
            "yigoid":"grid0",
            "style":{ "right": "50%","transform": "translateX(30px)"},
            "editable":true
        }
    },
    "foot": {
        "type": "element",
        "elementType": "SegementToolbar",
        "elementProps": {
            "showItems":["BillNew","BillEdit","BillSave","BillCancel","BillDelete"]
        }
    },
    "controls": {
        "grid0":{
            "control": "PlainGrid",
            "yigoid": "grid0",
            "cellWidth": "100",
            "cellWidths":{
                "PlantID":"150",
                "MaterialID":"50"
            }
        }
    }
}